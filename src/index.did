    getMessages: () -> (vec record {id:text; attachmentURL:text; title:text; body:text; createdAt:nat64; updatedAt:opt nat64}) query;
    updateMessage: (text, record {attachmentURL:text; title:text; body:text}) -> (opt record {id:text; attachmentURL:text; title:text; body:text; createdAt:nat64; updatedAt:opt nat64});
